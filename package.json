{
  "name": "breaker-ai",
  "version": "0.0.5",
  "description": "CLI to scan prompts for injection risks",
  "scripts": {
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "dev": "ts-node src/cli/index.ts",
    "prepublishOnly": "npm run test && npm run build",
    "lint": "eslint . --ext .ts,.js,.mjs,.cjs",
    "pub": "npm run prepublishOnly && npm publish --access public --tag next"
  },
  "bin": {
    "breaker": "dist/cli/index.js"
  },
  "keywords": [
    "breaker",
    "ai",
    "prompt",
    "injection",
    "security",
    "breaker-ai",
    "cli",
    "prompt-security",
    "prompt-injection",
    "prompt-safety",
    "prompt-risk",
    "prompt-scanner",
    "prompt-checker",
    "prompt-validator"
  ],
  "author": "Breaker AI",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^22.14.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.1.1"
  }
}